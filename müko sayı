

<?php

function perfonum($N)
{

    $sum = 0;

    for ($i = 1; $i < $N; $i++)
    {
        if ($N % $i == 0)
        {
            $sum = $sum + $i;
        }
    }


    return $sum == $N;
}


$N = 100;

if (perfonum($N))
    echo " mükosayı";
else
    echo "mekmükosayı";
?>


alert ("prfonum");
